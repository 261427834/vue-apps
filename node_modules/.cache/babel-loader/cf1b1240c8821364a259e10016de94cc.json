src=\"item.idView\" class=\"image\">\r\n      </el-carousel-item>\r\n    </el-carousel>\r\n</template>\r\n\r\n<script>\r\n  export default{\r\n    name:'zmd',\r\n    data(){\r\n      return {\r\n        imagebox:[{id:0,idView:require('../assets/maincover.png')},\r\n        // {id:1,idView:require('../assets/imagebox/2.jpg')},\r\n        // {id:2,idView:require('../assets/imagebox/3.jpg')}\r\n        ],\r\n         // 图片父容器高度\r\n        zmdHeight :700,\r\n        // 浏览器宽度\r\n        screenWidth :0\r\n      }\r\n    },\r\n     methods:{\r\n              setSize:function () {\r\n                // 通过浏览器宽度(图片宽度)计算高度\r\n                this.bannerHeight = 400 / 1920 * this.screenWidth;\r\n              },\r\n            },\r\n          mounted() {\r\n              // 首次加载时,需要调用一次\r\n              this.screenWidth =  window.innerWidth;\r\n              this.setSize();\r\n              // 窗口大小发生改变时,调用一次\r\n              window.onresize = () =>{\r\n              this.screenWidth =  window.innerWidth;\r\n              this.setSize();\r\n            }\r\n          }\r\n  }\r\n</script>\r\n\r\n<style>\r\n   .el-carousel__item h3 {\r\n      color: #475669;\r\n      font-size: 14px;\r\n      opacity: 0.75;\r\n      line-height: 200px;\r\n      margin: 0;\r\n    }\r\n\r\n    .el-carousel__item:nth-child(2n) {\r\n      background-color: #99a9bf;\r\n    }\r\n\r\n    .el-carousel__item:nth-child(2n+1) {\r\n      background-color: #d3dce6;\r\n    }\r\n    img{\r\n     /*设置图片宽度和浏览器宽度一致*/\r\n      width:100%;\r\n      height:inherit;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}